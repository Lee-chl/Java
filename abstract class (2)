public class Grandeur extends Car {

	@Override
	public void start() {
		System.out.println( " Grandeur 의 시동을 겁니다.");
	}

	@Override
	public void drive() {
		System.out.println( " Grandeur 를 운전합니다.");
	}

	@Override
	public void stop() {
		System.out.println("Grandeur를 멈춥니다.");
	}

	@Override
	public void turnoff() {
		System.out.println("Grandeur의 시동을 끕니다.");
	}

	@Override
	public void showCar() {
		System.out.println("이 차는 Grandeur 입니다.");
	}
	

}
